    <title>Ferreteria</title>
  </head>
  <body>

    <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
        <div class="container-fluid">
          <a class="navbar-brand" href="/">ColiMateriales</a>
          <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
          </button>
          <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav me-auto mb-2 mb-lg-0">
              <li class="nav-item">
                <a href="/employes" class="nav-link active" aria-current="page" href="#">Empleados</a>
              </li>
              <li class="nav-item">
                <a href="/storage" class="nav-link active" href="#">Inventario</a>
              </li>
              <li class="nav-item">
                <a href="/supplier" class="nav-link active" href="#">Proveedores</a>
              </li>
              <li class="nav-item">
                <a href="/sales" class="nav-link active" href="#">Punto de Venta</a>
              </li>
            </ul>
            <form class="d-flex">
              <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
              <button class="btn btn-outline-success" type="submit">Search</button>
            </form>
          </div>
        </div>
      </nav>

    <div class="container">

        
        <h1 class="mt-4">Agregar Proveedor</h1>
        <div class="card border-secondary">
          <div class="card-body">
            <form action="/supplieradd" method="POST">
              <div class="mb-3">
    
                <label for="user" class="form-label">Nombre</label>
                <input type="text" class="form-control" id="nombre" name="nombre" tabindex="2">
    
                <label for="user" class="form-label">Folio</label>
                <input type="text" class="form-control" id="folio" name="folio" tabindex="3">
    
                <label for="user" class="form-label">Marca</label>
                <input type="text" class="form-control" id="marca" name="marca" tabindex="4">
    
                    <label for="user" class="form-label">Email</label>
                <input type="text" class="form-control" id="email" name="email" tabindex="5">
                
                <label for="user" class="form-label">Telefono</label>
                <input type="text" class="form-control" id="telefono" name="telefono" tabindex="6">
        
              </div>
              <button type="submit" class="btn btn-primary" tabindex="5">Agregar</button>
              <a href="/employes" class="btn btn-danger">Cancelar</a><br>
            </form>
          </div>
        </div>   
    </div>

 
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>

   
  </body>
